//WAP to print ascending order

import java.util.*;

public class Ascendingorder{
	
	public static void main(String[] args){
		
		int  length,i,j,p;
		
		Scanner sc=new Scanner(System.in);
		
		System.out.println("enter the length of given number ");
		
		length=sc.nextInt();
		
		//String length=Integer.toString(length);
		
		  // char ch[]= length.toCharArray(); 
		   
		   for(i=0;i<length;i++)
		   {
			   for(j=length;j>0;j--)
			   {
				   for(p=j;p>i;p--)
				   {
					   System.out.println(p);
				   }
			   }
		   }
	}
}
	   